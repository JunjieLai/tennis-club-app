{
  "name": "tennis-club-app",
  "version": "1.0.0",
  "description": "A full-stack tennis club management application",
  "main": "server/index.js",
  "scripts": {
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "setup-db": "node server/scripts/setup-database.js",
    "seed": "node server/scripts/generate-sample-data.js",
    "init": "node server/scripts/init-all.js"
  },
  "keywords": [
    "tennis",
    "club",
    "management",
    "react",
    "express"
  ],
  "author": "Your Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1"
  }
}
